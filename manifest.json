{
  "name": "BrainDump",
  "version": "1.0",
  "manifest_version": 3,
  "description": "Easily dump your ideas while browsing the internet",
  "author": "Koji",
  "permissions": ["storage"],

  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  }
}
